{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "NodeJS Starter Project",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "dist",
      "node_modules"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madeindra/nodejs-starter.git"
  },
  "keywords": [
    "nodejs",
    "rest",
    "api",
    "starter",
    "template"
  ],
  "author": "madeindra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madeindra/nodejs-starter/issues"
  },
  "homepage": "https://github.com/madeindra/nodejs-starter#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "mongodb": "^4.8.1",
    "multer": "^1.4.5-lts.1",
    "pino-http": "^8.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
